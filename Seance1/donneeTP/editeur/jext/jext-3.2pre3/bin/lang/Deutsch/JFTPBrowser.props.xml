   <property name="JFTPBrowserPlugin.status.message.noTextAreaSelected"      value="Fehler: Kein Textbereich markiert" />
   <property name="JFTPBrowserPlugin.status.message.hasntBeenSaved"          value="Fehler: Textbereich wurde nicht gespeichert" />
   
  <!-- Text for node  -->
   <property name="JFTPBrowserPlugin.nodeLabel.loading"        value="Lade Inhalt..." />
   <property name="JFTPBrowserPlugin.nodeLabel.interrupted"        value="unterbrochen..." />
   <property name="JFTPBrowserPlugin.nodeLabel.emptyFolder"        value="<empty>" />
   
</xproperties> 
